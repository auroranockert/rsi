fn <%= self.name %>(<%= self.arguments.map { |a| a.to_native_prototype }.compact.join(', ') %>)<%= self.render('function/native-prototype/results') %> {
  unsafe {
<%= self.children.map { |c| c.uses }.flatten.map { |p| "    use #{p};\n" }.join -%>
<%= self.children.map { |c| c.prelude }.compact.map { |p| "    #{p}\n" }.join -%>
    <%= (n = self.children.find { |x| x.foreign_result }) ? "let #{n.foreign_result} = " : '' %><%= self.foreign %>(<%= self.children.map { |c| c.to_foreign_argument }.compact.join(", ") %>);
<% case self.results.length
   when 0 -%>
<% when 1 -%>
    return <%= self.results[0].to_native_result %>;
<% else -%>
    return (<%= self.results.map { |r| r.to_native_result }.join(', ') %>);
<% end -%>
  }
}